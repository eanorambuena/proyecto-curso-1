{
  "name": "proyecto-curso-1-uaustralargentina",
  "version": "1.0.0",
  "description": "Proyecto del curso Diseñando páginas web con Bootstrap 4",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently 'npm run watch:scss' 'npm run dev'",
    "test": "echo 'Error: no test specified' && exit 1",
    "scss": "node-sass -o css/ css/",
    "less": "lessc css/styles.less css/styles.css",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:less": "onchange 'css/*.less' -- npm run less",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin me.html -d dist --htmlmin -o dist/me.html && usemin projects.html -d dist --htmlmin -o dist/projects.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin",
    "grunt": "grunt",
    "grunt:build": "grunt build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/eanorambuena1/proyecto-curso-1.git"
  },
  "author": "Emmanuel Norambuena",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/eanorambuena1/proyecto-curso-1/issues"
  },
  "homepage": "https://bitbucket.org/eanorambuena1/proyecto-curso-1#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.6.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "^0.0.7",
    "imagemin-cli": "^7.0.0",
    "jit-grunt": "^0.10.0",
    "less": "^4.1.3",
    "lite-server": "^2.6.1",
    "node-sass": "^8.0.0",
    "rimraf": "^4.1.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "concurrently": "^7.6.0",
    "jquery": "^3.6.3",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
